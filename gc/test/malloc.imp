int[] t1;
int[][] t2;
int[][][] t3;

main {
  t1 = malloc(int, 2);

  t1[0] = 0;
  t1[1] = 1;

  print(t1[0]); // 0
  print(t1[1]); // 1

  print(len(t1)); // 2

  //

  t2 = malloc(bool, 2, 2);

  t2[0][0] = false;
  t2[0][1] = true;
  t2[1][0] = false;
  t2[1][1] = true;

  print(t2[0][0]); // false
  print(t2[0][1]); // true
  print(t2[1][0]); // false
  print(t2[1][1]); // true

  print(len(t2)); // 2
  print(len(t2[0])); // 2
  print(len(t2[1])); // 2

  //

  t3 = malloc(int, 2, 2, 2);

  t3[0][0][0] = 0;
  t3[0][0][1] = 1;
  t3[0][1][0] = 2;
  t3[0][1][1] = 3;
  t3[1][0][0] = 4;
  t3[1][0][1] = 5;
  t3[1][1][0] = 6;
  t3[1][1][1] = 7;

  print(t3[0][0][0]); // 0
  print(t3[0][0][1]); // 1
  print(t3[0][1][0]); // 2
  print(t3[0][1][1]); // 3
  print(t3[1][0][0]); // 4
  print(t3[1][0][1]); // 5
  print(t3[1][1][0]); // 6
  print(t3[1][1][1]); // 7

  print(len(t3)); // 2
  print(len(t3[0])); // 2
  print(len(t3[1])); // 2
  print(len(t3[0][0])); // 2
  print(len(t3[0][1])); // 2
  print(len(t3[1][0])); // 2
  print(len(t3[1][1])); // 2
}
